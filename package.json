{
  "name": "theme-dark-monokai",
  "version": "0.1.0",
  "description": "A dark Monokai theme extension for JupyterLab",
  "author": "Itay Dafna",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab-theme"
  ],
  "license": "BSD-3-Clause",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typedoc": "~0.15.4",
    "typescript": "~3.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themePath": "style/index.css"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/ibdafna/theme-dark-monokai/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ibdafna/theme-dark-monokai"
  }
}
